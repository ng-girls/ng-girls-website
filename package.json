{
  "name": "ng-girls-page",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --stats-json",
    "postbuild": "npm run scully -- --scanRoutes && npm run sw-build && npm run static",
    "static": "cp -r static/* dist/static/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "scully": "scully",
    "scully:serve": "scully serve",
    "sw-build": "node scripts/workbox-build.js && npx rollup -c",
    "new-event": "ng g @scullyio/init:post --metaDataFile='templates/workshop-meta.yaml' --target=workshops",
    "new-post": "ng g @scullyio/init:post --metaDataFile='templates/post-meta.yaml' --target=blog"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.7",
    "@angular/cdk": "^11.0.3",
    "@angular/common": "~9.1.7",
    "@angular/compiler": "~9.1.7",
    "@angular/core": "~9.1.7",
    "@angular/forms": "~9.1.7",
    "@angular/material": "^11.0.3",
    "@angular/platform-browser": "~9.1.7",
    "@angular/platform-browser-dynamic": "~9.1.7",
    "@angular/router": "~9.1.7",
    "@scullyio/init": "0.0.28",
    "@scullyio/ng-lib": "latest",
    "@scullyio/scully": "latest",
    "ngx-page-scroll": "^7.0.4",
    "ngx-page-scroll-core": "^7.0.4",
    "rxjs": "~6.5.4",
    "scully-plugin-disable-angular": "^3.1.1",
    "tslib": "^1.10.0",
    "workbox-core": "^5.1.3",
    "workbox-google-analytics": "^5.1.3",
    "workbox-precaching": "^5.1.3",
    "workbox-routing": "^5.1.3",
    "workbox-strategies": "^5.1.3",
    "workbox-window": "^5.1.3",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.6",
    "@angular/cli": "~9.1.6",
    "@angular/compiler-cli": "~9.1.7",
    "@angular/language-service": "~11.0.5",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "~5.4.3",
    "rollup": "^2.10.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.3.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3",
    "workbox-build": "^5.1.3"
  }
}
