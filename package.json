{
  "name": "ng-girls-page",
  "version": "0.0.0",
  "engines": {
    "node": "12.13.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "prebuild": "ng build --stats-json --prod",
    "build": "npm run scully -- --scanRoutes && npm run sw-build && npm run static",
    "postbuild": " npm run optimize",
    "static": "cp -r static/* dist/static/ && cp static/.htaccess dist/static/.htaccess",
    "optimizeOld": "node scripts/optimize.js",
    "optimize": "npx ts-node scripts/prepare.ts",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "clean": "rm -f scully.ng-girls-page.config.js",
    "scully": "npm run clean &&  scully",
    "scully2": "scully",
    "psi": "node scripts/psi.ts",
    "psi:dev": "node scripts/psi.ts --dev",
    "psi:new": "rm tmp/*.json && node scripts/psi.ts --dev",
    "scully:serve": "scully serve",
    "scully:watch": "scully --watch",
    "sw-build": "node scripts/workbox-build.js && npx rollup -c",
    "new-event": "ng g @scullyio/init:post --metaDataFile='templates/workshop-meta.yaml' --target=workshops",
    "new-post": "ng g @scullyio/init:post --metaDataFile='templates/post-meta.yaml' --target=blog"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.11",
    "@angular/cdk": "^11.0.3",
    "@angular/common": "~11.2.11",
    "@angular/compiler": "~11.2.11",
    "@angular/core": "~11.2.11",
    "@angular/forms": "~11.2.11",
    "@angular/material": "^11.0.3",
    "@angular/platform-browser": "~11.2.11",
    "@angular/platform-browser-dynamic": "~11.2.11",
    "@angular/router": "~11.2.11",
    "@rx-angular/cdk": "^1.0.0-alpha.10",
    "@rx-angular/state": "^1.4.4",
    "@rx-angular/template": "^1.0.0-beta.28",
    "@scullyio/init": "0.0.28",
    "@scullyio/ng-lib": "1.1.1",
    "@scullyio/scully": "1.1.1",
    "ngx-page-scroll": "^7.0.4",
    "ngx-page-scroll-core": "^7.0.4",
    "rxjs": "~6.5.4",
    "scully-plugin-disable-angular": "^3.1.1",
    "tslib": "^2.0.0",
    "workbox-core": "^5.1.3",
    "workbox-google-analytics": "^5.1.3",
    "workbox-precaching": "^5.1.3",
    "workbox-routing": "^5.1.3",
    "workbox-strategies": "^5.1.3",
    "workbox-window": "^5.1.3",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.10",
    "@angular/cli": "~11.2.10",
    "@angular/compiler-cli": "~11.2.11",
    "@angular/language-service": "~11.2.11",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "dotenv": "^10.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "psi": "^4.1.0",
    "rollup": "^2.10.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.3.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.7",
    "webpack-bundle-analyzer": "^4.4.2",
    "workbox-build": "^5.1.3"
  }
}
